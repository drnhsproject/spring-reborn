    },
    {
      "name": "identityNumber",
      "type": "String"
    },
    {
      "name": "phoneNumber",
      "type": "String"
    },
    {
      "name": "photo",
      "type": "String",
      "validations": {
        "maxLength": 5000
      }
    },
    {
      "name": "address",
      "type": "String",
      "validations": {
        "maxLength": 5000
      }
    },
    {
